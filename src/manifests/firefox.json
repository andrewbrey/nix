{
	"$schema": "https://json.schemastore.org/webextension",
	"name": "__MSG_appName__",
	"version": "0.0.0",
	"manifest_version": 2,
	"description": "__MSG_appDescription__",
	"homepage_url": "https://github.com/andrewbrey/nix",
	"icons": {
		"16": "assets/icons/icon-16.png",
		"19": "assets/icons/icon-19.png",
		"38": "assets/icons/icon-38.png",
		"128": "assets/icons/icon-128.png",
		"150": "assets/icons/icon-150.png"
	},
	"default_locale": "en",
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"permissions": ["storage"],
	"browser_action": {
		"default_icon": {
			"16": "assets/icons/icon-16.png",
			"24": "assets/icons/icon-24.png",
			"32": "assets/icons/icon-32.png",
			"128": "assets/icons/icon-128.png"
		},
		"default_title": "Nix",
		"default_popup": "popup.html"
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Shift+Alt+N"
			},
			"description": "Open Nix"
		},
		"toggle_mute": {
			"suggested_key": {
				"default": "Shift+Alt+Down"
			},
			"description": "Toggle Nix Playback"
		}
	}
}
